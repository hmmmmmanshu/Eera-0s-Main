{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "eera-0s-main.vercel.app" }],
      "destination": "https://eera-os.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.eera-os.com" }],
      "destination": "https://eera-os.com/:path*",
      "permanent": true
    }
  ]
}

{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}












